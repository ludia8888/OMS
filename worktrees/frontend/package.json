{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "preview": "vite preview",
    "prepare": "husky",
    "analyze:types": "type-coverage",
    "analyze:unused": "knip",
    "analyze:deps": "depcheck",
    "analyze:circular": "madge --circular src",
    "analyze:bundle": "size-limit",
    "analyze:all": "npm run analyze:types && npm run analyze:unused && npm run analyze:deps && npm run analyze:circular",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@blueprintjs/core": "^5.19.0",
    "@blueprintjs/icons": "^5.22.0",
    "@blueprintjs/popover2": "^2.1.31",
    "@blueprintjs/select": "^5.3.20",
    "@blueprintjs/table": "^5.3.14",
    "@tanstack/react-query": "^5.80.6",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.9.0",
    "graphql": "^16.11.0",
    "immer": "^10.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.25.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/fragment-matcher": "^5.1.0",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@size-limit/preset-app": "^11.2.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "depcheck": "^1.4.7",
    "eslint": "^9.25.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "knip": "^5.60.2",
    "lint-staged": "^16.1.0",
    "madge": "^8.0.0",
    "prettier": "^3.5.3",
    "size-limit": "^11.2.0",
    "ts-prune": "^0.10.3",
    "type-coverage": "^2.29.7",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  }
}
